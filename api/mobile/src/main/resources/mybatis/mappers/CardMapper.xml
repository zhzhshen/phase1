<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sid.mobile.mapper.CardMapper">
    <select id="findByNumber" resultType="com.sid.mobile.model.Card">
        SELECT id, number, balance, data, calls, planId from card where number = #{number}
    </select>
</mapper>